{
    "request_title": "获取APP Access Token",
    "request_doc": "https://open.oceanengine.com/labels/7/docs/1713655428885516",
    "request_url": "https://open.oceanengine.com/open_api/oauth2/app_access_token/",
    "request_method": "POST",
    "request_params": {
        "app_id": {
            "type": "number",
            "description": "开发者申请的应用APP_ID，可通过“应用管理”界面查看",
            "required": 1,
            "level": 0,
            "children": {}
        },
        "secret": {
            "type": "string",
            "description": "开发者应用的私钥Secret，可通过“应用管理”界面查看（确保填入secret与app_id对应以免报错！）",
            "required": 1,
            "level": 0,
            "children": {}
        }
    },
    "response_params": {
        "code": {
            "type": "number",
            "description": "返回码,详见【附录-返回码】",
            "level": 0,
            "children": {}
        },
        "message": {
            "type": "string",
            "description": "返回信息,详见【附录-返回码】",
            "level": 0,
            "children": {}
        },
        "data": {
            "type": "json",
            "description": "json返回值",
            "level": 0,
            "children": {
                "access_token": {
                    "type": "string",
                    "description": "应用级别token",
                    "level": 1,
                    "children": {}
                },
                "expires_in": {
                    "type": "number",
                    "description": "access_token剩余有效时间,单位(秒)",
                    "level": 1,
                    "children": {}
                }
            }
        },
        "request_id": {
            "type": "string",
            "description": "请求日志id",
            "level": 0,
            "children": {}
        }
    }
}